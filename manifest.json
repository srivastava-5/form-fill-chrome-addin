{
  "name": "Finzy form filler",
  "description" : "Base Level Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "formFill.html",
    "default_icon": "extension_logo.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js":["formfill.js"]
    }
    
  ],
  "permissions": ["tabs","activeTab"]

}